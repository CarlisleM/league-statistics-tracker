CREATE TABLE teams (
    id INTEGER NOT NULL,
    name TEXT NOT NULL,
    league_id INTEGER NOT NULL REFERENCES leagues (id),
    PRIMARY KEY(id)
);

CREATE TABLE splits (
    id INTEGER NOT NULL,
    league_id INTEGER REFERENCES leagues (id) NOT NULL,
    PRIMARY KEY (id)
);

CREATE TABLE games (
    id INTEGER NOT NULL,
    league_id INTEGER REFERENCES leagues (id),
    split_id INTEGER REFERENCES splits (id),
    game_date DATE NOT NULL,
    game_count INTEGER NOT NULL,
    blue_team TEXT NOT NULL,
    red_team TEXT NOT NULL,
    PRIMARY KEY(id)
);

CREATE TABLE match_data (
    first_blood TEXT,
    first_tower TEXT,
    first_dragon TEXT,
    first_inhibitor TEXT,
    first_baron TEXT,
    winner TEXT NOT NULL,
    loser TEXT NOT NULL,
    game_id INTEGER REFERENCES games (id) NOT NULL
);

CREATE TABLE match_odds (
    team_one TEXT,
    team_two TEXT,
    team_one_win FLOAT,
    team_two_win FLOAT,
    team_one_first_blood FLOAT,
    team_two_first_blood FLOAT,
    team_one_ft5_kills FLOAT,
    team_two_ft5_kills FLOAT,
    team_one_ft10_kills FLOAT,
    team_two_ft10_kills FLOAT,
    odd_kills FLOAT,
    even_kills FLOAT
);

