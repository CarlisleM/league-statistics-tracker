DELETE games, match_data FROM games INNER JOIN match_data ON games.id = match_data.game_id WHERE league_id = 8;

DELETE 
FROM match_data m
USING games g
WHERE m.game_id = g.id
  and g.league_id = 8;

select id, date, league_id from games order by date desc where league_id = 5;

